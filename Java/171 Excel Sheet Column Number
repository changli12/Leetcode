class Solution {
    public int titleToNumber(String s) {
        int n=s.length();//String获取长度使用的是length()方法
        int result=0;
        for(int i=0;i<n;i++)
        {
            char ch=s.charAt(i);//String获取字符的方法
            result=result*26+ch-64;//char可以自动转为int，result要*26
        }
        return result;
    }
}
